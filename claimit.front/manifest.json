{
    "manifest_version": 3,
    "name": "ClaimIt",
    "version": "1.0",
    "description": "ClaimIt Chrome Extension",
    "icons": {
      "16": "./assets/airdrop.png",
      "48": "./assets/airdrop.png",
      "128": "./assets/airdrop.png"
    },
    "action": {
      "default_popup": "index.html"
    },
    "permissions": ["storage", "activeTab"],
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["styles.css"]
    }
    ]
  }
  